{
  "name": "s3gw-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production --verbose",
    "test": "jest --no-cache --watch",
    "test:ci": "jest --no-cache",
    "lint:eslint": "ng lint",
    "lint:html": "htmllint src/app/**/*.html && html-linter --config html-linter.config.json",
    "lint:scss": "stylelint \"**/*.scss\"",
    "lint": "run-p -csl --aggregate-output lint:*",
    "fix:prettier": "prettier --write \"{src,e2e}/**/*.{ts,scss}\"",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:scss": "stylelint \"**/*.scss\" --fix",
    "fix": "run-p -csl --aggregate-output fix:*",
    "i18n": "npm run i18n:extract && npm run i18n:pull && npm run i18n:convert",
    "i18n:extract": "transloco-keys-manager extract --sort --replace",
    "upgrade": "ncu -u --target minor"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.11",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/localize": "^13.3.11",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/router": "~13.3.11",
    "@fontsource/lato": "^4.5.10",
    "@fontsource/roboto-mono": "^4.5.8",
    "@mdi/font": "6.9.96",
    "@ng-bootstrap/ng-bootstrap": "^12.1.2",
    "@ngneat/transloco": "^4.1.1",
    "aws-sdk": "^2.1237.0",
    "bootstrap": "5.2.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.6",
    "lodash": "^4.17.21",
    "ng-block-ui": "^3.0.2",
    "ngx-toastr": "^14.3.0",
    "rxjs": "~7.5.7",
    "tslib": "^2.4.0",
    "validator": "^13.7.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.4",
    "@angular-devkit/build-angular": "~13.3.9",
    "@angular-eslint/builder": "^13.5.0",
    "@angular-eslint/eslint-plugin": "^13.5.0",
    "@angular-eslint/eslint-plugin-template": "^13.5.0",
    "@angular-eslint/schematics": "^13.5.0",
    "@angular-eslint/template-parser": "^13.5.0",
    "@angular/cli": "~13.3.9",
    "@angular/compiler-cli": "~13.3.11",
    "@ngneat/transloco-keys-manager": "^3.4.2",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.186",
    "@types/node": "^12.20.55",
    "@types/validator": "^13.7.8",
    "@types/crypto-js": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.14",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "html-linter": "^1.1.1",
    "htmllint-cli": "^0.0.7",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-preset-angular": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "typescript": "4.6.4"
  }
}
